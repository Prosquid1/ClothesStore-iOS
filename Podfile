# Uncomment the next line to define a global platform for your project
use_frameworks!

def modelPods
  pod 'Alamofire'
  pod 'RealmSwift'
  pod 'SwiftyJSON'
end

def sharedUIPods
  pod 'FaveButton'
  pod 'SwiftEntryKit'
  pod 'ViewAnimator'
end

# Pods for ClothesStore
target 'ClothesStore' do
  modelPods
  sharedUIPods
end

# Pods for ClothesStoreUITests
target 'ClothesStoreUITests' do
  modelPods
  sharedUIPods
end



